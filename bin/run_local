#!/bin/sh

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"

. $DIR/secrets.config

echo ./build/smug \
    -slack-token="$slacktok" -slack-channel="$slackchan" \
    -irc-server="$ircserver" -irc-channel="$ircchan" -irc-nick="$ircnick"


./build/smug \
    -slack-token="$slacktok" -slack-channel="$slackchan" \
    -irc-server="$ircserver" -irc-channel="$ircchan" -irc-nick="$ircnick"
